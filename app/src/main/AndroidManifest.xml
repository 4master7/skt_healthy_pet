<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skthealthypet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ranking_Activity"></activity>
        <activity
            android:name=".List_CompletePopUp_Activity"
            android:screenOrientation="fullSensor"
            android:theme="@style/popupTheme" />
        <activity
            android:name=".List_PopUp_Activity"
            android:screenOrientation="fullSensor"
            android:theme="@style/popupTheme" />
        <activity
            android:name=".List_AddPopUp_Activity"
            android:screenOrientation="fullSensor"
            android:theme="@style/popupTheme" />
        <activity android:name=".Loding_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Input_Activity" />
        <activity android:name=".Choice_Activity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>